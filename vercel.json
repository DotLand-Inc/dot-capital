{
  "installCommand": "pnpm install",
  "buildCommand": "CI='' pnpm run build:webapp",
  "outputDirectory": "packages/webapp/dist",
  "rewrites": [
    {
      "source": "/api/:slug*",
      "destination": "https://dot-capital.vercel.app/api/:slug*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}