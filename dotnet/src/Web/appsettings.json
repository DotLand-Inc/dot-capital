{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Yarp": "Information"
    }
  },
  "ConnectionStrings": {
    "Dotland.DotCapital.WebApiDb": "Server=localhost;Port=33997;Database=dotcapital_system;User=root;Password=strongpassword123;",
    "DefaultConnection": "Server=localhost;Port=33997;Database=dotcapital_system;User=root;Password=strongpassword123;",
    "TenantConnection": "Server=localhost;Port=33997;Database=dotcapital_tenant_template;User=root;Password=strongpassword123;"
  },
  "Jwt": {
    "Secret": "OVERRIDE_WITH_USER_SECRETS_OR_ENV"
  },
  "NodeBackend": {
    "BaseUrl": "https://dot-capital.dotland.fr",
    "Timeout": 30
  },
  "PublicRoutes": [
    "/auth",
    "/health",
    "/api/meta"
  ],
  "ReverseProxy": {
    "Routes": {
      "catch-all": {
        "ClusterId": "node-backend",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "node-backend": {
        "Destinations": {
          "destination1": {
            "Address": "https://dot-capital.dotland.fr"
          }
        }
      }
    }
  },
  "AllowedHosts": "*"
}